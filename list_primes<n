def prime_below_n(n):
  if n <= 2:
    return []

  is_prime = [True] * n
  is_prime[0] = is_prime[1] = False 

  for p in range(2, int(n**0.5) + 1):
    if is_prime[p]:
    for multiple in range(p * p, n, p):
      is_prime[multiple] = False

  primes = [i for i, prime_status in enumerate(is_prime) if prime_status]
  return primes
